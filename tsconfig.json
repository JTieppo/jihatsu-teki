{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/test/page.jsx", "src/data.js", "src/app/ui/card.jsx", "src/app/ui/filtro.jsx", "src/app/login/usuario/[id]/[companyName]/page.jsx", "src/app/layout.jsx", "src/app/app/page.jsx", "src/app/api/busca/route.js"  ],
  "exclude": [
    "node_modules"
  ]
}
